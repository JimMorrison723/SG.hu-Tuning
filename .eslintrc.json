{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "globals": {
        "browser": true
    },
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "indent": [
            "error",
            2
        ],
        "no-var": [
            "warn"
        ],
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 1,
                "maxEOF": 1
            }
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}